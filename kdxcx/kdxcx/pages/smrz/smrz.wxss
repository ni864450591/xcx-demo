/* form */
.smrz-list{
    width: 100%;
}
.smrz-item{
    width: 100%;
    height: 108rpx;
    border-bottom: 1px solid #eee;
    background-color: #fff;
    display: flex;
    align-items: center;
}
.smrz-text{
    width: 160rpx;
    font-size: 28rpx;
    color: #999999;
    margin:0 0 0 80rpx;
}
.smrz-input{
    flex:1;
    font-size: 28rpx;
    color: #626161;
}
.btn{
    margin-top: 20rpx;
    widows: 660rpx;
    background-color: #35BD00;
    height: 72rpx;
    border-radius:40rpx;
    color:#fff;
    font-size: 28rpx;

}